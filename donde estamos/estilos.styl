body
	background #CCCCCC
	font-size 14px
	margin  0px
	text-align  center

header
	background  white
	display  inline-block
	font-size 10px
	margin 0.1em
	vertical-align top
	width 99%


header #logo
	display inline-block
	float left
	padding-left 0.2em
	position relative
	width 85%


header #logo img
	border-top-right-radius 10px
	border-top-left-radius 10px
	border-bottom-right-radius 10px
	border-bottom-left-radius 10px
	display inline-block
	float left
	margin 0.3em 0px
	width 75%

header #logo #titulo-item
	display block
	margin 0.4em 
	padding 0.1em 
	text-align left
	vertical-align top
	width 20%

header #logo #titulo-item h3
	margin 1em
	font-family "Sancreek", Arial
	text-align center
	width 96%

header #avatar
	display inline-block
	float right
	margin-top -17em
	position relative
	text-align left
	width 30%

header #avatar #titulo
	font-family "Sancreek", Arial
	font-size 1.7em
	float right
	margin 0.2em
	margin-top 0.1em
	position relative
	text-align center
	width 100%
	

nav
	background #684124
	border-bottom 4px solid transparent
	border-left 10px solid white
	border-top 4px solid transparent
	border-bottom 4px solid transparent
	border-right 10px solid white
	border-top 4px solid transparent
	color white
	display inline-block
	font-size 1.4em
	text-align left
	vertical-align top
	width 97%

nav ul
	list-style none
	margin 0px
	padding 0 0.8em

nav ul li 
	display inline-block
	margin 0 1.2em 0 0
	vertical-align top

nav ul li a 
	color white
	font-weight normal
	display block
	padding 0 0

nav ul li a:hover
	border-bottom 3px solid #0058A5
	margin 0px
	padding 0px

#contenedor
	background white
	display inline-block
	top 0px
	width 99%

#contenedor #imagen
	display none

#contenedor #mapa
	display inline-block
	float left
	margin 0.7em -2em 
	position relative
	width  50%

#contenedor #mapa  .mapa
	display inline-block
	width 80%

#contenedor #mapa  #texto
	font-size 1.1em
	font-weight normal
	margin 0.3em 5em
	text-align left
	width 70%

#contenedor #pasear
	display inline-block
	float left
	position relative
	margin 1em -0.4em
	text-align left
	width 30%

#contenedor #pasear h2
	font-family "Sancreek", Arial
	font-size 1.4em
	font-weight normal
	text-align center
	margin 0.3em
	margin-top 1px
	width 50%

#contenedor #informacion
	display inline-block
	float right
	margin 1em 1em
	width 20%

#contenedor #informacion h3
	background black
	color white
	font-family "Sancreek",Arial
	font-size 1.0em
	font-weight normal
	margin 0.2em 0.4em
	padding 0 0.4em
	text-align left
	width 90%

#contenedor #informacion #map
	display inline-block
	float right
	width 90%
#contenedor #informacion #map .map
	width 90%

#contenedor #informacion .logo
	display inline-block
	float right
	margin 0.4em 0.4em
	width 15%

#contenedor #informacion a 
	color black
	font-size 1.2em
	float left
	width 70%

#contenedor #informacion a:hover
	font-size 1.5em

#contenedor #informacion .wasp
	display inline-block
	margin 0.3em
	position relative
	width 15%

#contenedor #informacion #video
	display inline-block
	margin 0.5em
	width 90%

#contenedor #informacion #video video
	padding 0.3em
	width 99%

#contenedor #informacion #video a 
	font-size 1.3em
	font-weight normal
	color black

#contenedor #informacion #video a:hover 
	color #318C9F
	font-size 1.5em

#contenedor #informacion #enlaces
	display inline-block
	width 90%

#contenedor #informacion #enlaces h3
	font-size 0.9em
	font-weight normal
	text-align left

#contenedor #informacion #enlaces .imagenes
	display inline-block
	vertical-align top
	width 13%


#contenedor  #pie 
	background #321600
	border-bottom 4px solid transparent
	border-left 15px solid transparent
	border-top 4px solid white
	border-bottom 4px solid white
	border-right 15px solid transparent
	border-top 4px solid white
	color white
	display inline-block
	width 90%

@import "responsive.styl"