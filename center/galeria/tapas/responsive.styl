@media screen and (min-width:768px)
	body
		margin 0 auto
		max-width 1200px

	nav ul .boton
		font-size 0.8em
		font-family "Allan", Arial

	nav ul .boton a:hover
		background #FAFFBD
		border-radius 2px
		color #126513
		font-size 1.3em

	#contenedor
		box-shadow  2px 9px 15px #283C48

	#contenedor #imagenes
		width 57%

	#contenedor #datos #mapa .mapa
		border-radius 15px
		box-shadow 3px 2px 10px #283C48