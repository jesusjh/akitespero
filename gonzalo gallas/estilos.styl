body
	background #CCCCCC
	font-size 13px
	margin  0px
	text-align  center
	z-index 0px

header
	background  white
	display  inline-block
	margin 0.1em
	vertical-align top
	width 99%

header #logo
	display inline-block
	float left
	margin 0.3em
	width 50%

header #logo #logo-item
	display inline-block
	border-radius 20px
	padding 0.3em 0.4em
	width 94%
	
nav
	background #D8D8D8
	border-bottom 4px solid transparent
	border-left 20px solid white
	border-top 4px solid transparent
	border-bottom 20px solid white
	border-right 20px solid white
	border-top 4px solid transparent
	color white
	display inline-block
	font-size 1.7em
	text-align left
	vertical-align top
	width 97%

nav ul
	list-style none
	margin 0px
	padding 0 0.8em

nav ul li 
	display inline-block
	margin 0 1.2em 0 0
	vertical-align top

nav ul li a 
	color white
	font-weight normal
	display block
	padding 0 0

nav ul li a:hover
	border-bottom 6px solid #414141
	margin 0px
	padding 0px

header #nota
	display inline-block
	float right
	margin 0.3em
	top 0px
	width 30%

header #nota #nota-item
	display inline-block
	width 50%

header #nota #nota-item h3
	font-family "Sancreek", Arial
	font-size 1.1em
	font-weight normal
	text-align left
	width 100%

header #nota #avatar
	display inline-block
	width 50%

header #nota #avatar h3
	font-family "Sancreek", Arial
	font-size 1.1em
	font-weight normal
	text-align left
	width 100%

header #nota #avatar a
	color black
	font-size 1.0em

header #nota #avatar a:hover
	color #008FD5
	font-family "Libre Baskerville", Arial
	font-size 1.2em

#contenedor
	background white
	display inline-block
	width 95%

#contenedor #imagen
	border-bottom 1px solid black
	display inline-block
	margin 0.3em
	width 100%

#contenedor #imagen #img 
	border-radius 20px
	width 92%

#contenedor #texto
	border-bottom 2px solid black
	display inline-block
	width 100%

#contenedor #texto h2
	font-family "Sancreek", Arial
	font-size 1.6em

#contenedor #texto #texto-derecha
	background #EDF1F4
	float left
	margin 0.4em
	padding 0.4em
	width 30%

#contenedor #texto #texto-derecha .jpg
	border 2px solid black
	border-radius 25px
	width 50%

#contenedor #texto #texto-derecha p 
	text-align left

#contenedor #texto #texto-centro
	background #EDF1F4
	display inline-block
	margin 0.3em
	margin-left -3em
	width 30%

#contenedor #texto #texto-centro .jpg
	border 2px solid black
	border-radius 25px
	width 50%

#contenedor #texto #texto-centro p 
	text-align left

#contenedor #texto #texto-izquierda
	background #EDF1F4
	float right
	margin 0.4em
	padding 0.4em
	width 30%

#contenedor #texto #texto-izquierda .jpg
	border 1px solid black
	border-radius 25px
	width 50%

#contenedor #texto #texto-izquierda p 
	text-align left

#contenedor #informacion 
	padding 0px 0.9em
	width 100%

#contenedor #informacion h3
	font-family "Sancreek", Arial
	font-size 2.0em
	font-weight normal
	text-align center                                                               
	width 100%

#contenedor #texto-item
	border-top 1px solid black
	display inline-block
	margin 0.7em 0 0.9em
	vertical-align top
	width 100%

#contenedor #texto-item h3
	font-family "Sancreek", Arial
	font-size 1.2em
	font-weight normal
	text-align left
	padding 0 0.4em
	width 60%

#contenedor #texto-item .item-texto 
	display inline-block
	float left
	width 70%

#contenedor #texto-item .item-texto .image
	border-radius 30px
	float left
	padding 0.4em
	width 66%

#contenedor #texto-item .item-texto .imagen
	border-radius 20px
	float right
	padding 0.2em 0.4em
	width 30%

#contenedor #texto-item .item-texto 
	font-size 1.0em
	font-weight normal
	margin 0 0.4em
	text-align left
	width 40%

#contenedor #texto-item .item-texto #tapas-item
	display inline-block
	font-size 1.2em
	width 80%

#contenedor #texto-item .item-texto #tapas-item h3
	display inline-block
	font-size 1.6em
	padding 0 0.4em
	width 100%

#contenedor #texto-item  #texto-iten
	display inline-block
	float right
	width 45%

#contenedor #texto-item #texto-iten #tapas
	display inline-block
	font-size 1.5em
	font-weight normal
	width 90%

#contenedor #texto-item #texto-iten h3
	border-top 2px solid black
	font-size 1.3em
	text-align left
	width 93%

#contenedor #texto-item  #texto-iten #foto
	border-radius 30px
	display inline-block
	float right
	width 50%

#contenedor #pie
	background black
	display inline-block
	width 100%

#contenedor #pie #presentacion
	background black
	color white
	display inline-block
	font-size 1.2em
	margin 0.8em
	width 80%

#contenedor #pie  #enlaces
	float left
	width 30%

#contenedor #pie  #enlaces .redes
	border-radius 30px
	float left
	margin 0 0.4em 
	padding 0.4em
	position relative
	width 10%

#pie #enlace
	display inline-block
	float left
	width 30%

#pie #enlace .red
	float left
	padding 0.3em
	width 50%

#contenedor #pie #donde
	background red
	display inline-block
	float right
	margin 0.3em
	position relative
	width 30%

#contenedor #pie #donde .vamos
	width 40%

