@media screen and (min-width:768px)
	body
		margin 0 auto
		max-width 1200px

	#contenedor 
		box-shadow 	8px 9px 6px #2A2A2A

	#contenedor #imagen 
		box-shadow 0 9px 10px black
		display inline-block
		height auto
		margin 0
		top 0
		vertical-align top
		width 100%

	#contenedor #imagen .image
		border-radius 40%
		display inline-block
		height 150px
		margin 0
		position relative
		top 0
		vertical-align top
		width 99%

	#contenedor #lateral #video h3
		border-top-left-radius 5px
		border-top-right-radius 5px
		border-bottom-left-radius 5px
		border-bottom-right-radius 5px

