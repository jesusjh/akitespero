@media screen and (min-width:768px)
	body
		margin 0 auto
		max-width 1200px

	#contenedor 
		box-shadow 7px 3px 14px #283c48

	#contenedor #imagen
		box-shadow 0 9px 10px black
		border-radius 10px
		display inline-block
		height auto
		margin 0
		top 0
		vertical-align top
		width 100%

	#contenedor #imagen .image
		border-radius 40%
		display inline-block
		height 150px
		margin 0
		position relative
		top 0
		vertical-align top
		width 99%